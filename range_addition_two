from typing import List


class Solution:
    def maxCount(self, m: int, n: int, ops: List[List[int]]) -> int:
        matrix = []
        biggest_nums = []

        for i in range(m):
            current_row = []
            for j in range(n):
                current_row.append(0)
            matrix.append(current_row)

        for rc in ops:
            row, col = rc

            for i in range(row-1, -1, -1):
                for j in range(col-1, -1, -1):
                    matrix[i][j] += 1

        for i in range(m):
            for j in range(n):
                current_num = matrix[i][j]
                if not biggest_nums:
                    biggest_nums.append(current_num)
                elif biggest_nums[0] == current_num:
                    biggest_nums.append(current_num)
                elif biggest_nums[0] < current_num:
                    biggest_nums = [current_num]

        return len(biggest_nums)
